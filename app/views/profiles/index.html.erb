<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Avatar data</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Sex</th>
      <th>Dob</th>
      <th>Street</th>
      <th>Postcode</th>
      <th>State</th>
      <th>Country</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.user %></td>
        <td><%= profile.avatar_data %></td>
        <td><%= profile.first_name %></td>
        <td><%= profile.last_name %></td>
        <td><%= profile.sex %></td>
        <td><%= profile.DOB %></td>
        <td><%= profile.street %></td>
        <td><%= profile.postcode %></td>
        <td><%= profile.state %></td>
        <td><%= profile.country %></td>
        <td><%= profile.latitude %></td>
        <td><%= profile.longitude %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
